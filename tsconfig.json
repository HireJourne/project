{
  "include": ["src/**/*", "supabase/functions/**/*"],
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  }
}
